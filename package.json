{
    "name": "@kcdesign/editor",
    "description": "kcdesign editor",
    "version": "1.0.55",
    "private": false,
    "type": "module",
    "author": "kcaitech.com",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "publishConfig": {
        "registry": "https://packages.aliyun.com/6393d698d690c872dceedcc0/npm/npm-registry/"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development vite build --mode development --watch",
        "build-main": "vite build",
        "build-proto": "vite build --config ./vite.config-proto.ts",
        "build": "npm run build-main & npm run build-proto",
        "doc": "typedoc",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@kcdesign/data": "1.0.89",
        "element-plus": "2.3.0",
        "jszip": "^3.10.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "pathkit-wasm": "^1.0.0",
        "uuid": "^9.0.0",
        "vite-plugin-commonjs": "^0.10.1",
        "vue": "^3.2.13",
        "vue-i18n": "^9.2.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "@vitejs/plugin-vue": "^5.0.4",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^8.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "sass": "^1.77.6",
        "terser": "^5.31.0",
        "ts-jest": "^29.1.1",
        "typedoc": "^0.25.13",
        "typescript": "^5.4.5",
        "vite": "^5.2.8",
        "vite-plugin-arraybuffer": "^0.0.7",
        "vite-plugin-css-injected-by-js": "^3.5.1",
        "vite-plugin-dts": "^3.9.1",
        "vite-plugin-vue-devtools": "^7.0.25"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true,
            "amd": true,
            "vue/setup-compiler-macros": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended",
            "@vue/typescript"
        ],
        "parserOptions": {
            "parser": "@typescript-eslint/parser"
        },
        "rules": {
            "no-mixed-spaces-and-tabs": 0,
            "no-useless-escape": 0,
            "no-unused-vars": 0,
            "vue/multi-word-component-names": "off",
            "no-debugger": "off",
            "no-empty": "off",
            "prefer-const": "off"
        }
    },
    "eslintIgnore": [
        "**/vendor/raphael/*.js"
    ],
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "keywords": []
}
