{
  "name": "mossdesign",
  "description": "A design system for rapidly building web applications",
  "productName": "MossDesign",
  "version": "0.1.0",
  "private": true,
  "author": "moss.design",
  "main": "background.js",
  "publishConfig": {
    "registry": "https://packages.aliyun.com/6393d698d690c872dceedcc0/npm/npm-registry/"
  },
  "scripts": {
    "build-worker": "cd ./src/communication && npm install && npm run build",
    "dev:web": "npm run build-worker && vue-cli-service --public=moss.design serve",
    "build:web": "cross-env NODE_ENV=production && npm run build-worker && vue-cli-service build",
    "install-electron-deps": "electron-builder install-app-deps",
    "dev:electron": "npm run build-worker && vue-cli-service electron:serve",
    "build:electron": "npm run build-worker && vue-cli-service electron:build",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate-package-dev": "node generate-package.js dev && npm install",
    "generate-package-prod": "node generate-package.js prod && npm install",
    "electron:generate-icons": "electron-icon-builder --input=./src/assets/logo.png --output=build --flatten"
  },
  "dependencies": {
    "@kcdesign/data": "^0.3.69",
    "@vueuse/core": "^10.9.0",
    "ali-oss": "^6.18.1",
    "aws-sdk": "2.1420.0",
    "axios": "^1.3.6",
    "element-plus": "2.3.0",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "js-base64": "^3.7.7",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.6",
    "moment": "^2.29.4",
    "node-stream-zip": "*",
    "pathkit-wasm": "^1.0.0",
    "pinyin-match": "^1.2.4",
    "qrcode": "^1.5.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.58.3",
    "skia-canvas": "^1.0.1",
    "uuid": "^9.0.0",
    "vconsole": "^3.15.1",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.2.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^4.1.6",
    "vue-wxlogin": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-typescript": "^7.17.12",
    "@types/ali-oss": "^6.16.9",
    "@types/chai": "^4.3.1",
    "@types/jest": "^29.5.5",
    "@types/js-base64": "^3.3.1",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/qrcode": "^1.5.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/babel-plugin-jsx": "^1.2.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "^5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0",
    "chai": "^4.3.6",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "electron": "^20.3.12",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "~8.2.0",
    "typedoc": "^0.22.17",
    "typescript": "~4.5.5",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0",
    "yarn": "^1.22.19"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "amd": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-mixed-spaces-and-tabs": 0,
      "no-useless-escape": 0,
      "no-unused-vars": 0,
      "vue/multi-word-component-names": "off",
      "no-debugger": "off",
      "no-empty": "off",
      "prefer-const": "off"
    }
  },
  "eslintIgnore": [
    "**/vendor/raphael/*.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": []
}
